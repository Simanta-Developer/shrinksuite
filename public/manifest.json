{
  "manifest_version": 3,
  "name": "ShrinkSuite",
  "description": "Compress images, videos, and PDFs directly in your browser.",
  "version": "1.0",
  "action": {
    "default_title": "Open ShrinkSuite"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["sidePanel", "storage"],
  "side_panel": {
    "default_path": "src/sidepanel/index.html"
  },
  "icons": {

  }
}

